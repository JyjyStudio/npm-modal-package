	"license": "MIT",
	"author": "JyjyStudio",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JyjyStudio/npm-modal-package.git"
	},
	"keywords": [
		"modal",
		"react-modal-component",
		"modal-plugin",
		"simple-modal",
		"hrnet"
	],
	"bugs": {
		"url": "https://github.com/JyjyStudio/npm-modal-package/issues"
	},
	"devDependencies": {
		"@babel/core": "^7.20.5",
		"@storybook/addon-actions": "^6.5.14",
		"@storybook/addon-essentials": "^6.5.14",
		"@storybook/addon-interactions": "^6.5.14",
		"@storybook/addon-links": "^6.5.14",
		"@storybook/builder-webpack4": "^6.5.14",
		"@storybook/manager-webpack4": "^6.5.14",
		"@storybook/react": "^6.5.14",
		"@storybook/test-runner": "^0.9.1",
		"@storybook/testing-library": "^0.0.13",
		"@types/node": "18.11.9",
		"@types/react": "18.0.24",
		"@types/react-dom": "18.0.8",
		"@types/styled-components": "^5.1.26",
		"@types/testing-library__jest-dom": "^5.14.5",
		"babel-loader": "^8.3.0",
		"gh-pages": "^4.0.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-scripts": "^5.0.1",
		"styled-components": "5.3.6",
		"typescript": "4.9.3"
	},
	"peerDependencies": {
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"styled-components": "5.3.6"
	},
	"scripts": {
		"clean-lib": "rm -rf lib",
		"build-lib": "npm run clean-lib && tsc && cp package.json README.md ./lib",
		"predeploy-storybook": "npm run build-storybook",
		"deploy-storybook": "gh-pages -d storybook-static",
		"storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
		"build-storybook": "NODE_OPTIONS=--openssl-legacy-provider build-storybook",
		"test-storybook": "test-storybook",
		"test": "react-scripts test",
		"test:coverage": "react-scripts test --watchAll=false --coverage"
	},
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3"
	},
	"jest": {
		"coveragePathIgnorePatterns": [
			"<rootDir>/src/stories/",
			"<rootDir>/node_modules/"
		]
	}
}
